postgres=# CREATE TABLE students(id SERIAL PRIMARY KEY, name VARCHAR(100), email VARCHAR(100));
CREATE TABLE
postgres=# INSERT INTO students (name, email) VALUES ('Alymbek', 'alymbek@example.com'), ('Timur','timur@example.com'), ('Beka','beka@example.com');
INSERT 0 3
postgres=# SELECT name, email FROM students;
  name   |        email
---------+---------------------
 Alymbek | alymbek@example.com
 Timur   | timur@example.com
 Beka    | beka@example.com
(3 rows)


postgres=# SELECT * FROM students;
 id |  name   |        email
----+---------+---------------------
  1 | Alymbek | alymbek@example.com
  2 | Timur   | timur@example.com
  3 | Beka    | beka@example.com
(3 rows)


postgres=# SELECT name, email FROM students WHERE name = 'Timur';
 name  |       email
-------+-------------------
 Timur | timur@example.com
(1 row)


